{
    "name": "alexwenzel/laravel-commentary",
    "description": "A package for Laravel to post a comment on an article and to manage them.",
    "keywords": ["laravel","comments"],
    "authors": [
        {
            "name": "alexwenzel",
            "email": "alexander.wenzel.berlin@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/models"
        ],
        "psr-0": {
            "Alexwenzel\\LaravelCommentary\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
